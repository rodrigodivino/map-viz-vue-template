{
  "env": {
    "node": true
  },
  "extends": [
    "plugin:vue/vue3-recommended", //from eslint-plugin-vue
    "eslint:recommended", // from eslint
    "@vue/eslint-config-typescript/recommended", // from @vue/eslint-config-typescript
    "@vue/eslint-config-prettier" // from @vue/eslint-config-prettier
  ],
  "plugins": ["prettier"],
  "rules": {
    "quotes": ["warn", "double", { "avoidEscape": true }]
  }
}
